<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:local="clr-namespace:Launcher" x:Class="Launcher.MainWindow"     Title="Minecraft Launcher for Windows 10 - Forked from M Center"     Height="522"     Width="857.273"     Icon="unnamed.jpg"     WindowStartupLocation="CenterScreen"     Cursor="Pen"     AllowDrop="True"     IsTabStop="False"     ResizeMode="CanMinimize"     Drop="Window_Drop"     DragEnter="Window_DragEnter"     DragLeave="Window_DragLeave"     Closing="Window_Closing">    <Grid Name="root">        <Grid Name="home" Visibility="Visible">                    <Grid.Background>                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">                                <GradientStop Color="#FFD6A0A0" Offset="0.007"/>                                <GradientStop Color="#FFF30909" Offset="1"/>                        </LinearGradientBrush>                    </Grid.Background>                    <Grid.ColumnDefinitions>                        <ColumnDefinition Width="594*"/>                        <ColumnDefinition Width="2*"/>                        <ColumnDefinition Width="255*"/>                    </Grid.ColumnDefinitions>                    <Border BorderBrush="#FF000000"                         BorderThickness="1"                         HorizontalAlignment="Left"                         Margin="10,82,0,10"                         Width="336">                            <Border.Background>                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">                                        <GradientStop Color="#FFE8E7E7" Offset="0"/>                                        <GradientStop Color="#FFE6E6E6" Offset="1"/>                                        <GradientStop Color="#FFD8D5D5" Offset="0.42"/>                                        <GradientStop Color="#FFAEADAD" Offset="0.813"/>                                </LinearGradientBrush>                            </Border.Background>                            <StackPanel HorizontalAlignment="Left"                                 Height="381"                                 Margin="9,9,0,0"                                 VerticalAlignment="Top"                                 Width="316">                                    <Button Name="play"                                         Content="Launch Minecraft"                                         HorizontalAlignment="Left"                                         Height="40"                                         Margin="10,10,0,0"                                         VerticalAlignment="Top"                                         Width="296"                                         Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"                                         FontSize="24"                                         ToolTip="Play minecraft with unlock full game fix"                                         Cursor="Hand"                                         Click="play_Click"/>                                    <Button Name="hack_disable"                                         Content="Disable patch"                                         HorizontalAlignment="Left"                                         Height="40"                                         Margin="10,10,0,0"                                         VerticalAlignment="Top"                                         Width="296"                                         Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"                                         FontSize="24"                                         ToolTip="Disables the hacks of unlock full game"                                         Cursor="Hand"                                         Click="hack_disable_Click"/>                                    <Button Name="manager_enable"                                         Content="Unfreeze Minecraft"                                         HorizontalAlignment="Left"                                         Height="40"                                         Margin="10,10,0,0"                                         VerticalAlignment="Top"                                         Width="296"                                         Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"                                         FontSize="24"                                         ToolTip="Click on this if you are stuck on Minecraft loading screen"                                         Cursor="Hand"                                         Click="manager_enable_Click"/>                                    <Button Name="manager_disable"                                         Content="Disable manager"                                         HorizontalAlignment="Left"                                         Height="40"                                         Margin="10,10,0,0"                                         VerticalAlignment="Top"                                         Width="296"                                         Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"                                         FontSize="24"                                         ToolTip="Disables the server. Click on this if your computer gets slow."                                         Cursor="Hand"                                         Click="manager_disable_Click"/>                                    <Button Name="install"                                         Content="Install/Update Minecraft"                                         HorizontalAlignment="Left"                                         Height="40"                                         Margin="10,10,0,0"                                         VerticalAlignment="Top"                                         Width="296"                                         Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"                                         FontSize="24"                                         ToolTip="Install and Update Minecraft from official server"                                         Cursor="Hand"                                         Click="install_Click"/>                                    <Button Name="install2"                                         Content="Install custom package"                                         HorizontalAlignment="Left"                                         Height="40"                                         Margin="10,10,0,0"                                         VerticalAlignment="Top"                                         Width="296"                                         Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"                                         UseLayoutRounding="False"                                         FontSize="24"                                         ToolTip="Install custom or patched .appx package, at any location"                                         Cursor="Hand"                                         Click="install2_Click"/>                            </StackPanel>                    </Border>                    <Border BorderBrush="#FF000000"                         BorderThickness="1"                         Margin="351,82,10,10"                         Grid.ColumnSpan="3">                            <Border.Background>                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">                                        <GradientStop Color="#FFE8E7E7" Offset="0"/>                                        <GradientStop Color="#FFE6E6E6" Offset="1"/>                                        <GradientStop Color="#FFD8D5D5" Offset="0.42"/>                                        <GradientStop Color="#FFAEADAD" Offset="0.813"/>                                </LinearGradientBrush>                            </Border.Background>                            <Grid>                                <RichTextBox Name="home_log"                                     Margin="9,24,9,71"                                     UndoLimit="5"                                     AllowDrop="False"                                     IsReadOnly="True"                                     VerticalScrollBarVisibility="Auto"                                     ToolTip="displays the log">                                        <FlowDocument AllowDrop="False"                                             Focusable="False"                                             FontSize="14">                                                <Paragraph>                                                    <Run Text="Welcome to Minecraft Launcher for Windows 10, a free and open source launcher brought to you by trungnt2910."/>                                                </Paragraph>                                        </FlowDocument>                                </RichTextBox>                                <Button Name="error_copy"                                     Margin="10,357,275,10"                                     Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"                                     Content="Copy last error to clipboard"                                     ToolTip="Copy the last error you recieved so that you can share with developers on discord"                                     Cursor="Hand"                                     Click="error_copy_Click"/>                                <Button Name="a_play"                                     Margin="275,357,10,10"                                     Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"                                     Content="Try different mode"                                     ToolTip="It does nothing right now"                                     Cursor="Hand"                                     Click="a_play_Click"/>                                <ProgressBar Name="progress"                                     Margin="154,3,10,370"                                     Visibility="Hidden"/>                                <TextBlock Name="filer"                                     Margin="154,3,10,370"                                     Text="hgkkyg"                                     AllowDrop="False"                                     Visibility="Hidden"/>                                <Border Name="par"                                     BorderBrush="#FF000000"                                     BorderThickness="1"                                     Margin="154,3,10,370"                                     Visibility="Hidden"/>                                <TextBox Name="cmdprompt"                                     Margin="10,0,10,42"                                     TextWrapping="Wrap"                                     Height="29"                                     VerticalAlignment="Bottom"                                     Background="#FFEEEFF1"                                     Foreground="#FF8B92A4"                                     KeyDown="TextBox_KeyDown"                                     KeyUp="cmdprompt_KeyUp"/>                            </Grid>                    </Border>                    <Image HorizontalAlignment="Left"                         Height="52"                         Margin="206,10,0,0"                         VerticalAlignment="Top"                         Width="51"                         Source="unnamed.jpg"/>                    <TextBlock Grid.ColumnSpan="3"                         Margin="262,10,224,431"                         TextWrapping="Wrap"                         Text="Minecraft Launcher"                         FontSize="36"                         Foreground="#FFFFFFFF"/>                    <Button Name="Homepage"                         Grid.ColumnSpan="3"                         Margin="262,10,224,431"                         Opacity="0"                         Cursor="Hand"                         Click="Homepage_Click"/>            </Grid>            <Grid Name="drop" Visibility="Hidden">            <Grid.Background>                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">                    <GradientStop Color="#FFD6A0A0" Offset="0.007"/>                    <GradientStop Color="#FFF30909" Offset="1"/>                </LinearGradientBrush>            </Grid.Background>        </Grid>        </Grid></Window>